@page "/pacienteEditar/{id}"

<h2 class="text-radzen-primary mb-2">Editar Paciente</h2>

@if (_pacienteModel is null)
{
    <span>Cargando <i class="fas fa-spinner fa-spin"></i></span>
}
else
{
    <CascadingValue Value="_isBussy">
        <PacienteForm PacienteRequest="_pacienteModel" OnSubmit="EditPaciente"/>
    </CascadingValue>
}